{
  "name": "dnb-wm-si-web-tech-front-end-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "firebase": "^9.12.1",
    "postcss": "^8.4.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "tailwind": "^4.0.0",
    "typescript": "^4.8.4",
    "uuid": "9.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/uuid": "^8.3.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "additional-rule": "warn",
      "indent": [
        "error",
        4
      ],
      "quotes": [
        "error",
        "single"
      ],
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "semi": [
        "error"
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "additional-typescript-only-rule": "off",
          "additional-rule": "off"
        }
      }
    ]
  }
}
